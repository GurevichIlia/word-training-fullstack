{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "current-language",
      "urls": [
        "https://word-training-ngrx.oa.r.appspot.com/api/auth/getCurrentUser",
        "https://word-training-ngrx.oa.r.appspot.com/api/languages/getCurrentLanguage",
        "https://word-training-ngrx.oa.r.appspot.com/api/word-group/getAllGroups",
        "https://word-training-ngrx.oa.r.appspot.com/api/vocabulary/getAllWords",
        "https://kit.fontawesome.com/0cef43f628.js",
        "https://fonts.googleapis.com/css?family=Roboto:300,400,500&amp;display=swap",
        "https://fonts.googleapis.com/icon?family=Material+Icons",
        "https://fonts.googleapis.com/css2?family=Muli:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "20m",
        "strategy": "freshness"
      }
    }
  ]
}
